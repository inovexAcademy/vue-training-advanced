{
  "name": "vue-training",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "engineStrict": true,
  "engines": {
    "node": "20 || 22"
  },
  "workspaces": [
    "_examples/*",
    "_exercises/*",
    "_solutions/*"
  ],
  "scripts": {
    "start": "pnpm run dev -ws -if-present",
    "dev": "vite -ws -if-present",
    "build": "run-p type-check build-only -ws -if-present",
    "test:unit": "vitest run",
    "test:unit:ci": "run-p type-check:test \"test:unit --run --coverage\" -ws -if-present",
    "test:component": "playwright install && playwright test -c ./tests/component/playwright-ct.config.ts -ws -if-present",
    "test:integration": "playwright install && playwright test -c ./tests/integration/playwright.config.ts -ws -if-present",
    "build-only": "vite build -ws -if-present",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false -ws -if-present",
    "type-check:test": "vue-tsc --noEmit -p tsconfig.app.json --composite false -ws -if-present",
    "lint": "eslint .",
    "lint:fix": "pnpm run lint --fix",
    "lint:ci": "pnpm run lint --format json --output-file coverage/eslint.report.json -ws -if-present",
    "format": "prettier --write . -ws -if-present",
    "format:check": "prettier --check . -ws -if-present",
    "storybook": "storybook dev -p 6006 -ws -if-present",
    "prepare": "simple-git-hooks -ws -if-present",
    "preinstall": "npx only-allow pnpm"
  },
  "dependencies": {
    "@fontsource-variable/source-code-pro": "^5.2.5",
    "@fontsource-variable/source-sans-3": "^5.2.7",
    "@sit-onyx/icons": "1.0.0-beta.23",
    "@vueuse/core": "^13.7.0",
    "oidc-client-ts": "^3.2.1",
    "pinia": "^3.0.3",
    "pinia-plugin-persistedstate": "^4.5.0",
    "sit-onyx": "1.0.0-beta.285",
    "vue": "^3.5.20",
    "vue-component-type-helpers": "^3.0.6",
    "vue-i18n": "^11.1.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.31.0",
    "@import-meta-env/cli": "~0.7.3",
    "@import-meta-env/typescript": "~0.4.0",
    "@import-meta-env/unplugin": "~0.6.2",
    "@intlify/unplugin-vue-i18n": "^6.0.8",
    "@pinia/testing": "^1.0.1",
    "@playwright/experimental-ct-vue": "~1.52.0",
    "@playwright/test": "~1.52.0",
    "@storybook/addon-actions": "^8.6.12",
    "@storybook/addon-essentials": "^8.6.12",
    "@storybook/vue3": "^8.6.12",
    "@storybook/vue3-vite": "^8.6.12",
    "@tsconfig/node22": "^22.0.1",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.15.17",
    "@vitejs/plugin-vue": "^5.2.4",
    "@vitest/coverage-v8": "^3.1.3",
    "@vue/compiler-dom": "^3.5.13",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "~0.7.0",
    "@yao-pkg/pkg": "^6.4.1",
    "dotenv": "^16.5.0",
    "eslint": "^9.31.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-vue": "^10.3.0",
    "globals": "^16.3.0",
    "jsdom": "^26.1.0",
    "lint-staged": "^16.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.5.3",
    "prettier-plugin-organize-imports": "^4.1.0",
    "prettier-plugin-sort-json": "^4.1.1",
    "sass-embedded": "^1.88.0",
    "simple-git-hooks": "^2.13.0",
    "storybook": "^8.6.12",
    "storybook-dark-mode": "^4.0.2",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.38.0",
    "vite": "^6.3.5",
    "vite-plugin-run": "~0.6.1",
    "vite-plugin-vue-devtools": "^7.7.7",
    "vitest": "^3.1.3",
    "vitest-mock-extended": "^3.1.0",
    "vitest-sonar-reporter": "^2.0.1",
    "vue-router": "^4.5.1",
    "vue-tsc": "^2.2.10",
    "z-vue-scan": "^0.0.35"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": [
      "prettier --write -u",
      "eslint --fix"
    ]
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@import-meta-env/unplugin",
      "esbuild",
      "simple-git-hooks"
    ]
  },
  "packageManager": "pnpm@10.15.1+sha512.34e538c329b5553014ca8e8f4535997f96180a1d0f614339357449935350d924e22f8614682191264ec33d1462ac21561aff97f6bb18065351c162c7e8f6de67"
}
